{
  "name": "adapt-inspector",
  "version": "1.0.2",
  "framework": ">=2.0.16",
  "displayName": "Inspector",
  "extension": "inspector",
  "targetAttribute": "_inspector",
  "description": "An extension to inspect details of elements",
  "main": "/js/adapt-inspector.js",
  "keywords": [
    "adapt-plugin",
    "adapt-extension"
  ],
  "authors": [
    "Tom Greenfield",
    "Daryl Hedley"
  ],
  "license": "GPL-3.0",
  "homepage": "https://github.com/cgkineo/adapt-inspector",
  "bugs": "https://github.com/cgkineo/adapt-inspector/issues",
  "private": true,
  "devDependencies": {
    "@semantic-release/commit-analyzer": "^9.0.2",
    "@semantic-release/git": "^10.0.1",
    "@semantic-release/github": "^8.0.5",
    "@semantic-release/npm": "^9.0.1",
    "@semantic-release/release-notes-generator": "^10.0.3",
    "conventional-changelog-eslint": "^3.0.9",
    "semantic-release": "^19.0.3"
  },
  "scripts": {
    "postversion": "cp package.json bower.json"
  },
  "release": {
    "plugins": [
      [
        "@semantic-release/commit-analyzer",
        {
          "preset": "eslint"
        }
      ],
      [
        "@semantic-release/release-notes-generator",
        {
          "preset": "eslint"
        }
      ],
      "@semantic-release/npm",
      "@semantic-release/github",
      [
        "@semantic-release/git",
        {
          "assets": [
            "package.json",
            "bower.json"
          ],
          "message": "Chore(release): ${nextRelease.version} [skip ci]\n\n${nextRelease.notes}"
        }
      ]
    ]
  }
}
